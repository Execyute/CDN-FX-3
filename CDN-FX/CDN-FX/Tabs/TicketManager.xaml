<UserControl x:Class="CDN_FX.TicketManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CDN_FX"
             xmlns:lControls="clr-namespace:CDN_FX.UserControls"
             xmlns:lEControls="clr-namespace:CDN_FX.UserControls.Entries"
             xmlns:lOControls="clr-namespace:CDN_FX.UserControls.Tabs.Option"
             xmlns:lTControls="clr-namespace:CDN_FX.UserControls.Tabs.Tableview"
             mc:Ignorable="d" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             d:DesignHeight="640" d:DesignWidth="1287"
             >
    <Grid>

        <!-- Tickets -->
        <lEControls:TMEntries x:Name="ucEntries" Margin="10 10 0 0"/>

        <!-- Catergory View -->
        <lControls:AppCategory x:Name="ucCategory" Margin="10 287 0 10"/>

        <!-- Tableview -->
        <lTControls:TMTable x:Name="ucTable" Margin="253 10 253 10" />

        <!-- Options -->
        <lOControls:TMOption x:Name="ucOption" Margin="0 10 10 0"/>

        <!-- Download -->
        <lControls:Downloader x:Name="ucDownloader" Margin="0 287 10 10"/>

    </Grid>
</UserControl>
